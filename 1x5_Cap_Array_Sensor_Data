#include <CapacitiveSensor.h>
int s0_1 = 6;
int s1_1 = 5;
int s2_1 = 4;
int En_1 = 3;

int Z_pin = A2;

long value1_1 = 0;
long value1_2 = 0;
long value1_3 = 0;
long value1_4 = 0;
long value1_5 = 0;

CapacitiveSensor cs = CapacitiveSensor(A0,Z_pin);

void selectChannel_1(int chnl) {
  int A = bitRead(chnl,0);
  int B = bitRead(chnl,1);
  int C = bitRead(chnl,2);
  digitalWrite(s0_1, A);
  digitalWrite(s1_1, B);
  digitalWrite(s2_1, C);
}


void setup() 
{
   pinMode(s0_1, OUTPUT);
   pinMode(s1_1, OUTPUT);
   pinMode(s2_1, OUTPUT);
   pinMode(En_1, OUTPUT);

   pinMode(Z_pin, OUTPUT);

   digitalWrite(s0_1, LOW);
   digitalWrite(s1_1, LOW);
   digitalWrite(s2_1, LOW);
   digitalWrite(En_1, LOW);

   cs.set_CS_AutocaL_Millis(0xFFFFFFFF);     // turn off autocalibrate on channel 1 - just as an example
   Serial.begin(9600);
}

void loop() {
  for (int pin_1=0; pin_1<5; pin_1++) 
  {
    selectChannel_1(pin_1);
     if (pin_1 == 0) {
//        Serial.print(cs.capacitiveSensor(10));
        value1_1 = map(cs.capacitiveSensor(10),200,14000,0,1000);
        Serial.print(value1_1);
        Serial.print("\t");
      }
      if (pin_1 == 1) {
//        Serial.print(cs.capacitiveSensor(10));
        value1_2 = map(cs.capacitiveSensor(10),0,4000,0,1000);
        Serial.print(value1_2);
        Serial.print("\t");
      }
      if (pin_1 == 2) {
//        Serial.print(cs.capacitiveSensor(10));
        value1_3 = map(cs.capacitiveSensor(10),0,4000,0,1000); 
        Serial.print(value1_3);
        Serial.print("\t");
      }
      if (pin_1 == 3) {
//        Serial.print(cs.capacitiveSensor(10));
        value1_4 = map(cs.capacitiveSensor(10),0,4500,0,1000);
        Serial.print(value1_4);
        Serial.print("\t");
      }
      
      if (pin_1 == 4) {
//        Serial.print(cs.capacitiveSensor(10));
        value1_5 = map(cs.capacitiveSensor(10),300,14000,0,1000);
        Serial.print(value1_5);
        Serial.print("\t");
      }
  }
  Serial.println();
}